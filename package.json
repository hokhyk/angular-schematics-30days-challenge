{
  "name": "schematics30",
  "version": "0.2.0",
  "description": "This project is for Schematics 30days challenge of iT-Help",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "g": "npx -p dtsgenerator dtsgen src/hello-world/schema.json -o src/hello-world/schema.d.ts",
    "g:ngAdd": "npx -p dtsgenerator dtsgen src/ng-add/schema.json -o src/ng-add/schema.d.ts"
  },
  "keywords": [
    "angular",
    "schematics",
    "it-help",
    "30days"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/leochen0818/angular-schematics-30days-challenge"
  },
  "author": "Leo Chen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leochen0818/angular-schematics-30days-challenge/issues"
  },
  "homepage": "https://github.com/leochen0818/angular-schematics-30days-challenge",
  "schematics": "./src/collection.json",
  "ng-update": {
    "migrations": "./src/migration.json"
  },
  "dependencies": {
    "@angular-devkit/core": "^8.1.2",
    "@angular-devkit/schematics": "^8.1.2",
    "@schematics/angular": "^8.3.2",
    "@types/jasmine": "^3.3.9",
    "@types/node": "^8.0.31",
    "jasmine": "^3.3.1",
    "typescript": "~3.4.3"
  }
}
